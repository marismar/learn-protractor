  "repository": {
    "type": "git",
    "url": "https://github.com/marismarcosta/learn-protractor"
  },
  "keywords": [
    "protractor",
    "testing",
    "e2e"
  ],
  "author": "Marismar Costa",
  "license": "ISC",
  "devDependencies": {
    "faker": "^5.5.3",
    "protractor": "^7.0.0",
    "protractor-helper": "^4.2.1",
    "standard": "^16.0.3"
  }
}
